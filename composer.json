{
    "name": "sg/calendarbundle",
    "type": "symfony-bundle",
    "description": "This Bundle integrates the jQuery FullCalendar plugin into your Symfony2 application. Compatible with Doctrine ORM.",
    "keywords": ["Symfony2", "Calendar", "Event"],
    "homepage": "https://github.com/stwe/CalendarBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "stwe"
        }
    ],
    "support": {
        "issues": "https://github.com/stwe/CalendarBundle/issues"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "arshaw/fullcalendar",
                "version": "1.6.1",
                "dist": {
                    "type": "zip",
                    "url": "http://arshaw.com/fullcalendar/downloads/fullcalendar-1.6.1.zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "arshaw/fullcalendar": "1.6.1",
        "twitter/bootstrap": "2.3.*"
    },
    "autoload": {
        "psr-0": { "Sg\\CalendarBundle": "" }
    },
    "target-dir": "Sg/CalendarBundle"
}

